{
  "Name": "feedback-weekly-report-schedule",
  "Description": "Agenda semanal para geração de relatórios de feedback",
  "ScheduleExpression": "cron(0 8 ? * MON *)",
  "ScheduleExpressionTimezone": "America/Sao_Paulo",
  "State": "ENABLED",
  "FlexibleTimeWindow": {
    "Mode": "OFF"
  },
  "Target": {
    "Arn": "${LAMBDA_FUNCTION_ARN}",
    "RoleArn": "${EVENTBRIDGE_ROLE_ARN}",
    "Input": "{\"source\": \"aws.events\", \"detail-type\": \"Scheduled Event\", \"detail\": {\"reportType\": \"WEEKLY_REPORT\"}}"
  }
}
